{
  "name": "@jerni/store-mongodb",
  "version": "1.0.5",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "engines": {
    "bun": "1.2.8"
  },
  "publishConfig": {
